{
    "name": "God Tier Dev Status",
    "image": "god-tier-dev-status-container",
    "mounts": [
        "source=D:\\llms,target=/workspaces/notes/llms,type=bind"
    ],
    "runArgs": [
        "--gpus", "all"
    ],
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-toolsai.datawrangler",
                "ms-toolsai.jupyter",
                "ms-toolsai.vscode-jupyter-cell-tags",
                "ms-toolsai.jupyter-keymap",
                "ms-toolsai.jupyter-renderers",
                "ms-toolsai.vscode-jupyter-slideshow",
                "ms-python.python",
                "ms-python.debugpy",
                "ms-azuretools.vscode-docker"
            ]
        }
    }
}
